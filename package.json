{
  "name": "@soursopjs/core",
  "version": "0.1.0",
  "description": "The soursop core",
  "main": "src/main.js",
  "files": ["src/**/*", "dist/**/*"],
  "scripts": {
    "clean": "rm -rf build",
    "test": "npm-run-all build test:unit test:spec",
    "test:unit": "mocha './test/*.test.js'",
    "test:watch": "mocha './test/*.test.js' --watch-files src",
    "test:spec": "mocha './test/*.spec.js'",
    "build": "npm-run-all clean build:prod",
    "build:prod": "rollup --config config/rollup.config.prod.js",
    "build:dev": "rollup --config config/rollup.config.dev.js",
    "server": "node test/start-server.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soursopjs/soursop-core.git"
  },
  "keywords": [
    "gui",
    "javascript",
    "browser",
    "core"
  ],
  "author": "David256",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soursopjs/soursop-core/issues"
  },
  "homepage": "https://github.com/soursopjs/soursop-core#readme",
  "devDependencies": {
    "bootstrap": "^4.5.3",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "rollup": "^2.34.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
